int main()
{
  printf("%d\n", true);
  printf("%d\n", false);
  printf("%d\n", true && true);
  printf("%d\n", true && false);
  printf("%d\n", false && true);
  printf("%d\n", false && false);
  printf("%d\n", true || true);
  printf("%d\n", true || false);
  printf("%d\n", false || true);
  printf("%d\n", false || false);
  printf("%d\n", !false);
  printf("%d\n", !true);
  printf("%d\n", -10);
  printf("%d\n", --42);
  return 0;
}
